{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request",
        "percentage": 50
      },
      "enableDependencyTracking": true
    },
    "logLevels": {
      "default": "Information",
      "Function.HttpTrigger1": "Warning",
      "Function.ListContainers": "Warning",
      "Host": "Error"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  }
}
